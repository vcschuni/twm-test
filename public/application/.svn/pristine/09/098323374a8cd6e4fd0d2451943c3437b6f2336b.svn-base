																units: "metric", // choices are: "metric","imperial","degrees". default is "metric"
																steps: 4, // number of scale bar divisions (applies only if bar=true). default is 4
																text: true, // set to true to display current scale ratio (applies only if bar=true). default is false
																minWidth: 240 // width of scaleline in pixels. default is 64		
															},
	map: {
		default: {
			centre: {
				latitude: 54.5,
				longitude: -123.0
			},
			zoom: 5
		},
		zoom: {
			min: 4,
			max: 17
		},
		enableRotation: true,				// Allows Alt+Shift+Drag to rotate map [optional] defaults to false if 
		layers: [
		
			//
			// WMS Layer Configuration Example:
			//
			new ol.layer.Tile({
				title: "MoTT Districts", 								// TWM added property used by TWM and it's numerous plugins [required]
				hideFromLayerController: false,							// TWM added property to prevent layer from being added to the layer control [optional] - defaults to false
				preventIdentify: false,									// TWM added property to prevent layer from being identified [optional] - defaults to false
				type: "overlay",										// TWM added property used by TWM and it's numerous plugins [required]
				withCredentials: true,									// Make cross-site Access-Control requests using credentials [optional - default is false]
				removable: true,										// TWM added property used by LayerController plugin to allow layer to be removed [optional - defaults to false]
				legendImageUrl: "configuration/tdpa/img/my-legend.png", // Optional legend image override
				metadataUrls: [											// TWM added array to store metadata links.  If this layer is a WMS, TWM tries to populate this on it's own [optional]
					{Format: "text/plain", OnlineResource: "https://catalogue.data.gov.bc.ca/dataset/bc-highway-cams", type: "TC211"}
				],
				visible: false,											// Default OpenLayers property determining the layer's visible state [required]
				source: new ol.source.TileWMS({							// Default OpenLayers object defining it's source [required]
					url: "../path-to-geoserver/ows",									// Path (absolute or relative) [required]
					params: {
						LAYERS: "hwy:DSA_DISTRICT_BOUNDARY",					// Namespace and Layer Name [required]
						CQL_FILTER: "CONTRACT_AREA_NUMBER IN (18,19)" 			// Filter [optional]
					},
					transition: 0
				}),
				fields: [												// TWM added object used by TWM and it's numerous plugins [optional]
					{
						name: "DISTRICT_NUMBER",														// Actual field name [required]
						searchable: true,																// Whether this field is searchable by the ubersearch [optional - defaults to false]
						title: true,																	// Whether this field is used to define the title [optional]
						appendToField: "another_field_name",											// Used to append the value of this field to another.  Useful for addresses [optional]
						nameTransform: function(name) { return false; },								// Javascript function used to transform the field name. Field name is passed to the function. If boolean false is returned, the field name will not be shown [optional]
						valueTransform: function(value, feature) { return "<b>" + value + "</b>"; },	// Javascript function used to transform the field value. Field value is passed to the function. If boolean false is returned, entire field record will not be shown [optional].  2nd arg is the actual feature.
						timeFilterStart: "2021-04-01",													// Specifies the lower limit of a Date-time range slider in the LayerController to filter records by this field's values in a date range between this values and timeFilterEnd. 
						visible: false,																	// Hides the field from the popup and the 	
						timeFilterEnd: Date(),															// Specifies the upper limit of a Date-time range slider in the LayerController to filter records by this field's values in a date range between this values and timeFilterStart.  
						searchCriteria: [																// Search criteria for LayerSearch plugin.  Can include more than 1. [optional] - See VIP for complex example
							{
								name: "Northern Districts",
								type: "boolean",
								cql: "{{field}} IN (7,8,9)"
							}
						]
					},
					{
						//
						// This is a extra field not found the returned data (identified by NOT specifying a "name" property).  
						//
						// It's an example of how one would add a row in either the Identify2Popup or Identify2Tab results 
						// that shows a distance calculation (on the fly) of the users current location to the feature.
						//
						// In this case, the "entire feature" is passed as the second arg to the function.
						//
						// See VIP for this in action
						//
						nameTransform: function(name) { return false; }, // i.e. do not show name...
						valueTransform: function(value, feature) {
							// Set default result
							var result = "Could not calculate distance to facility";
							
							// Function to handle a geolocation result
							var gotGeoLocation = function(geoLoc) {
								if (geoLoc.length == 2) {
									if (feature.geometry && feature.geometry.coordinates) {
										// Transform feature coords to 4326
										var featureLoc = ol.proj.transform(feature.geometry.coordinates, app.map.getView().getProjection().getCode(), "EPSG:4326");

										// Query the DataBC Router
										$.ajax({
											url: "https://router.api.gov.bc.ca/directions.json",
											data: {
												criteria: "fastest",
												points: geoLoc[0]+","+geoLoc[1]+","+featureLoc[0]+","+featureLoc[1],
												roundTrip: false,
												apikey: "6097f62f6a8144edae53c59fc7c12351"
											},
											timeout: 15000
										})
										
										// Handle a success
										.done(function(response) {
											if (response.distance && response.distance > 0) {
												result = "<button class='btn btn-sm btn-success' type='button' onclick='navigateToUsingDefaultApp("+geoLoc[1]+","+geoLoc[0]+","+featureLoc[1]+","+featureLoc[0]+");'>Navigate to ("+response.distance.toFixed()+" km away)</button>";
											}
											$("#"+md5(feature.id)).html(result);
										})
										
										// Handle a failure
										.fail(function(jqxhr, settings, exception) {
											$("#"+md5(feature.id)).html(result);
										});
									} else {
										$("#"+md5(feature.id)).html(result);
									}
								} else {
									$("#"+md5(feature.id)).html(result);
								}
							}
							
							// Try to get the users location
							getGeoLocation(gotGeoLocation);

							// Return and empty span (to be populated later)
							return "<span id='"+md5(feature.id)+"'><div style='height:31px;line-height:31px;'>Calculating distance <image src='data:image/gif;base64,' /></div></span>";
						}
					}					
					
				]
			}),
			
			//
			// XYZ Tile Layer Configuration Example:
